    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/drivers": {
            "get": {
                "description": "fetch every driver available.",
                "consumes": [
                    "*/*"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "drivers"
                ],
                "summary": "Get all drivers.",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/models.Driver"
                            }
                        }
                    }
                }
            }
        },
        "/health": {
            "get": {
                "description": "get the status of server.",
                "consumes": [
                    "*/*"
                ],
                "produces": [
                    "text/plain"
                ],
                "tags": [
                    "health"
                ],
                "summary": "Show the status of server.",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        }
    },
    "definitions": {
        "models.Badge": {
            "type": "object",
            "properties": {
                "color": {
                    "type": "string"
                },
                "label": {
                    "type": "string"
                }
            }
        },
        "models.CurrentSeason": {
            "type": "object",
            "properties": {
                "fastestLaps": {
                    "type": "integer"
                },
                "podiums": {
                    "type": "integer"
                },
                "pointScored": {
                    "type": "integer"
                },
                "polePositions": {
                    "type": "integer"
                },
                "wins": {
                    "type": "integer"
                }
            }
        },
        "models.Driver": {
            "type": "object",
            "properties": {
                "age": {
                    "type": "integer"
                },
                "badges": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.Badge"
                    }
                },
                "bio": {
                    "type": "string"
                },
                "country": {
                    "type": "string"
                },
                "currentSeason": {
                    "$ref": "#/definitions/models.CurrentSeason"
                },
                "dateOfBirth": {
                    "type": "string"
                },
                "fastestLaps": {
                    "type": "integer"
                },
                "fullBiography": {
                    "type": "array",
                    "items": {
